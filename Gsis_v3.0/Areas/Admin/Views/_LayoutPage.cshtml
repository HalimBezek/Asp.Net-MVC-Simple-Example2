 <!DOCTYPE html>
<html lang="en" class="no-js">

<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- CSS -->
    <link href="/Areas/Admin/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="/Areas/Admin/assets/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="/Areas/Admin/assets/css/main.min.css" rel="stylesheet" type="text/css">
    <script type="text/javascript" src="/Areas/Admin/assets/js/jquery-1.7.2.min.js"></script>
</head>
<body class="dashboard">
  
    <div class="wrapper">

     

        <div class="top-bar">
            <div class="container">
                <div class="row">

                    <div class="col-md-2 logo">
                        <a href="/Admin">
                            <img src="/Areas/Admin/assets/img/kingboard-logo-white.png" alt="Gsis v1.0" />
                        </a>
                        <h1 class="sr-only">GüvenSoft</h1>
                    </div>

                    <div class="col-md-10">
                        <div class="row">
                            <div class="col-md-3">

                                <div class="input-group searchbox">
                                    <input type="search" class="form-control" placeholder="Aranacak Kelime...">
                                    <span class="input-group-btn">
                                        <button class="btn btn-default" type="button">
                                            <i class="fa fa-search"></i>
                                        </button>
                                    </span>
                                </div>
                            </div>
                            <div class="col-md-9">
                                <div class="top-bar-right">
                                    <a href="#" class="hidden-md hidden-lg main-nav-toggle"><i class="fa fa-bars"></i></a>
                                    <div class="notifications">
                                        <ul>

                                            <li class="notification-item inbox">
                                                <div class="btn-group">
                                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                                        <i class="fa fa-envelope"></i>
                                                        <span class="count">0</span>
                                                        <span class="circle"></span>
                                                    </a>
                                                    <ul class="dropdown-menu" role="menu">
                                                        <li class="notification-header">
                                                            <em>0 Adet Mesaj</em>
                                                        </li>
                                                        <li class="notification-footer">
                                                            <a href="#">Tüm Mesajları Listele</a>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </li>

                                            <li class="notification-item general">
                                                <div class="btn-group">
                                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                                        <i class="fa fa-bell"></i>
                                                        <span class="count">0</span>
                                                        <span class="circle"></span>
                                                    </a>
                                                    <ul class="dropdown-menu" role="menu">
                                                        <li class="notification-header">
                                                            <em>0 Adet İşlem</em>
                                                        </li>
                                                       
                                                        <li class="notification-footer">
                                                            <a href="#">Tüm İşlem Listesi</a>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </li>

                                        </ul>
                                    </div>

                                    <div class="logged-user">
                                        <div class="btn-group">
                                            <a href="#" class="btn btn-link dropdown-toggle" data-toggle="dropdown">
                                                @*<img src="/assets/img/user-avatar.png" />*@
                                                <span class="name">Admin</span>
                                                <span class="caret"></span>
                                            </a>
                                            <ul class="dropdown-menu" role="menu">
                                                @*<li>
                                                    <a href="#">
                                                        <i class="fa fa-user"></i>
                                                        <span class="text">Profile</span>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#">
                                                        <i class="fa fa-cog"></i>
                                                        <span class="text">Settings</span>
                                                    </a>
                                                </li>*@
                                                <li>
                                                    <a href="/Admin/cikis">
                                                        <i class="fa fa-power-off"></i>
                                                        <span class="text">Çıkış Yap</span>
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="bottom">
            <div class="container">
                <div class="row">
                    <div class="col-md-2 left-sidebar">
                        <nav class="main-nav">
                            <ul class="main-menu">
                                <li>
                                    <a href="/Admin"><i class="fa fa-dashboard fa-fw"></i><span class="text">Ana Sayfa</span></a>
                                </li>



                                <li>
                                    <a href="/Admin/kategoriler/listele">
                                        <i class="fa fa-clipboard fa-fw"></i><span class="text">Kategoriler</span>

                                    </a>

</li>
                          
                               <li>
                                   <a href="/Admin/manset/listele">
                                       <i class="fa fa-list-alt fw"></i><span class="text">Manşet Resimleri</span>

                                   </a>
                                   
                                </li>

                                <li>
                                    <a href="/Admin/referanslar/listele">
                                        <i class="fa fa-puzzle-piece fa-fw"></i><span class="text">Referanslar</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/Admin/galeri/listele">
                                        <i class="fa fa-puzzle-piece fa-fw"></i><span class="text">Galeri</span>
                                    </a>
                                </li>


                                <li>
                                    <a href="/Admin/haber/listele">
                                        <i class="fa fa-table fw"></i><span class="text">Haberler</span>
                                    </a>
                                </li>

                           

                              


                                <li>
                                    <a href="/Admin/form/listele">
                                        <i class="fa fa-edit fw"></i><span class="text">İletişim Formları</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="/Admin/ayarlar"><i class="fa fa-font fa-fw"></i><span class="text">Site Ayarları</span></a>
                                </li>
                            </ul>
                        </nav>
                   
                        <div class="sidebar-minified js-toggle-minified">
                            <i class="fa fa-angle-left"></i>
                        </div>
                      
                        <div class="sidebar-content">
                            <div class="panel panel-default">
                                <div class="panel-heading">
                                    <h5><i class="fa fa-lightbulb-o"></i> Bilgi</h5>
                                </div>
                                <div class="panel-body">
                                    <p>Kontrol panel v1.0 versiyonu yüklenmiştir. Yeni modüller ve özellikler bu pencereden takip edilebilir..</p>
                                </div>
                            </div>
                            <h5 class="label label-default"><i class="fa fa-info-circle"></i> Site Bilgileri</h5>
                            <ul class="list-unstyled list-info-sidebar bottom-30px">
                                <li class="data-row">
                                    <span class="data-name">Url</span>
                                    <span class="data-value">Gsis v1.0</span>
                                </li>
                             
                                <li class="data-row">
                                    <span class="data-name">Db</span>
                                    <span class="data-value">MsSql</span>
                                </li>
                                <li class="data-row">
                                    <span class="data-name">Framework</span>
                                    <span class="data-value">.Net 4.5</span>
                                </li>
                            </ul>
                        </div>
                 
                    </div>





                    @if (ViewBag.mesaj != null)
                    {
                        <div class="alert @ViewBag.mesaj[0] top-general-alert">
                            <span><b> @ViewBag.mesaj[1] </b></span>
                            <a href="" type="button" class="close">&times;</a>
                        </div>
                    }


                    @RenderBody()
                </div>
            </div>
        </div>
        <footer class="footer">
            &copy; 2014 GüvenSoft Yazılım Ltd Şti | Gsis Kontrol Panel v1.0 - www.guvensoft.com.tr
        </footer>

    </div>


          

    <script type="text/javascript" src="/Areas/Admin/assets/js/bootstrap.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/modernizr.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/king-common.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/stat/jquery.easypiechart.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/raphael-2.1.0.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/stat/flot/jquery.flot.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/stat/flot/jquery.flot.resize.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/stat/flot/jquery.flot.time.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/stat/flot/jquery.flot.pie.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/stat/flot/jquery.flot.tooltip.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/jquery.sparkline.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/datatable/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/datatable/jquery.dataTables.bootstrap.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/jquery.mapael.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/maps/usa_states.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/king-chart-stat.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/king-table.min.js"></script>
    <script type="text/javascript" src="/Areas/Admin/assets/js/king-components.min.js"></script>


