@{
    ViewBag.Title = "İletişim Formu Oku";
    Layout = "~/Areas/Admin/Views/_LayoutPage.cshtml";

    Gsis.Models.ILETISIM_FORMLARI form = (Gsis.Models.ILETISIM_FORMLARI)ViewBag.form;

}

<div class="col-md-10 content-wrapper">
    <div class="row">
        <div class="col-lg-4 ">
            <ul class="breadcrumb">
                <li><i class="fa fa-home"></i><a href="#">Ana Sayfa</a></li>
                <li class="active">Formlar</li>
            </ul>
        </div>

    </div>


    <div class="content">
        <div class="main-content">
            <div class="row">
                <div class="col-md-12">

                    <div class="widget">
                        <div class="widget-header">
                            <h3> Ad Soyad : @form.AD_SOYAD  </h3>
                        </div>
                        <div class="widget-content">
                            <p><b>Konu :</b> @form.KONU</p>
                            <p><b>Mail :</b> @form.MAIL</p>
                            <p><b>Gsm :</b> @form.GSM</p>
                            <hr />
                            <p><b>Mesaj :</b> @form.MESAJ</p>

                        </div>
                        <div class="widget-footer">
                            <b>Tarih :</b>   @form.TARIH
                        </div>
                    </div>
                    <a href="/Admin/form/listele" class="btn btn-warning">Geri Dön</a>
                    <a href="/Admin/form/listele?sil=@form.ID" class="btn btn-danger">Formu Sil</a>
                </div>


            </div>
        </div>
    </div>
</div>
